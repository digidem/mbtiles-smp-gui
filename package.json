{
  "description": "A GUI for converting MBTiles to SMP maps",
  "keywords": [
    "electron",
    "mbtiles",
    "smp",
    "gui",
    "typescript",
    "react"
  ],
  "homepage": "https://github.com/digidem/mbtiles-smp-gui#readme",
  "bugs": {
    "url": "https://github.com/digidem/mbtiles-smp-gui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/mbtiles-smp-gui.git"
  },
  "license": "MIT",
  "author": {
    "name": "Luandro",
    "email": "luandro@digital-democracy.org"
  },
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "npx concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:dll": "npx cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true npx webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "build:main": "npx cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true npx webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "npx cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true npx webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "postinstall": "node .erb/scripts/check-native-dep-cjs.js --force && npx electron-builder install-app-deps && npm run build:dll",
    "lint": "npx cross-env NODE_ENV=development npx eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "node ./.erb/scripts/clean-cjs.js",
    "package": "npm run clean && npm run build && npm run fix:sqlite && npx electron-builder build --publish never && npm run build:dll",
    "package:mac": "npm run clean && npm run build && npm run fix:sqlite && npx electron-builder build --mac --publish never && npm run build:dll",
    "package:win": "npm run clean && npm run build && npm run fix:sqlite && npx electron-builder build --win --publish never && npm run build:dll",
    "package:linux": "npm run clean && npm run build && npm run fix:sqlite && node ./.erb/scripts/linux-electron-rebuild.js && npx electron-builder build --linux --publish never && npm run build:dll",
    "fix:sqlite": "node ./.erb/scripts/fix-sqlite3-bug.js",
    "rebuild": "npx electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "npx ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "npx cross-env NODE_ENV=development npx electronmon -r ts-node/register/transpile-only .",
    "start:preload": "npx cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true npx webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "npx cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true npx webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "npx cross-env NODE_OPTIONS=--experimental-vm-modules npx jest -c jest.config.js",
    "test:mbtiles": "npx jest -c tests/jest.config.js",
    "prepare": "husky install"
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts",
      "./.erb/scripts/jest-setup.js"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist",
      ".erb/dll"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!chalk|ansi-styles|strip-ansi|ansi-regex|#ansi-styles|#supports-color)"
    ]
  },
  "dependencies": {
    "@mapbox/tiletype": "^0.3.1",
    "archiver": "^7.0.1",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.6.4",
    "fs-extra": "^11.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.5.3",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@electron/rebuild": "^3.7.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.0",
    "@svgr/webpack": "^8.1.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/archiver": "^6.0.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^29.5.14",
    "@types/node": "20.17.32",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/react-test-renderer": "^19.1.0",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.9.1",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "concurrently": "^9.1.2",
    "core-js": "^3.42.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "detect-port": "^2.1.0",
    "electron": "^26.6.10",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.1",
    "electronmon": "^2.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.1.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "mbtiles-reader": "^1.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "react-refresh": "^0.17.0",
    "react-test-renderer": "^19.1.0",
    "rimraf": "^6.0.1",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.4",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.7",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^5.10.0"
  },
  "build": {
    "productName": "MBTiles-to-SMP",
    "appId": "org.digidem.mbtiles-to-smp",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "icon": "assets/icon.icns",
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "icon": "assets/icon.png",
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "digidem",
      "repo": "mbtiles-smp-gui"
    }
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
